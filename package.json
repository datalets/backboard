{
  "name": "dribdat-backboard",
  "version": "0.1.10",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "moment": "^2.29.1",
    "vue": "^3.2.13",
    "vue-flip-countdown": "^0.0.3",
    "vue3-markdown-it": "^1.0.10"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/compiler-sfc": "^3.2.13",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "execa": "^5.1.1",
    "vue-collapsed": "^1.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "babelOptions": {
        "configFile": "./.babelrc",
        "babelrc": true
      }
    },
    "rules": {}
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "vue",
    "vuejs",
    "starter"
  ],
  "description": "Backboard SPA for dribdat"
}
